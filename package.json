  "license": "MIT",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonythetiger323/MayThe4thBeRussov2.git"
  },
  "keywords": [
    "wedding",
    "TypeScript",
    "React",
    "Material-UI"
  ],
  "bugs": {
    "url": "https://github.com/tonythetiger323/MayThe4thBeRussov2/issues",
    "email": "admin@maythe4thberusso.com"
  },
  "homepage": "http://www.maythe4thberusso.com",
  "scripts": {
    "test": "cross-env NODE_ENV=test PORT=8080 MYSQL_URL=mysql://localhost:33065/may_the_4th_be_russo_test node_modules/.bin/nyc node node_modules/jest/bin/jest.js",
    "watch:test": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start:dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.ts",
    "start:prod": "cross-env NODE_ENV=production node index.ts",
    "bs": "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "clean": "rimraf dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "tslint client server",
    "lint:fix": "tslint client server --fix",
    "lint-md": "remark .",
    "lint-md:fix": "remark-stringify",
    "lint-css": "stylelint",
    "lint-css:fix": "stylelint --fix",
    "type-check": "cd client && tsc && cd ../server && tsc"
  },
  "heroku-run-build-script": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
