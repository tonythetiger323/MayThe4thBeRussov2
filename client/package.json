    "@sentry/browser": "^5.6.2",
    "axios": "^0.19.0",
    "cloudinary-react": "^1.1.4",
    "formik": "^2.0.1",
    "google-map-react": "^1.1.4",
    "history": "^4.9.0",
    "lodash": "^4.17.15",
    "material-ui-chip-input": "^1.0.0",
    "material-ui-formik-components": "^0.4.0",
