  "license": "MIT",
  "engines": {
    "node": ">9.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonythetiger323/MayThe4thBeRussov2.git"
  },
  "keywords": [
    "wedding",
    "TypeScript",
    "React",
    "Material-UI"
  ],
  "bugs": {
