{
  "name": "may-the-4th-be-russo",
  "version": "1.0.0",
  "description": "Wedding website for Megan Bailey & Randy Russo",
  "author": "Tony Greeley",
  "license": "MIT",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonythetiger323/MayThe4thBeRussov2.git"
  },
  "keywords": [
    "wedding",
    "TypeScript",
    "React",
    "Material-UI"
  ],
  "bugs": {
    "url": "https://github.com/tonythetiger323/MayThe4thBeRussov2/issues",
    "email": "admin@maythe4thberusso.com"
  },
  "homepage": "http://www.maythe4thberusso.com",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rm -rf dist/",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "nodemon --config nodemon-prod.json dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "nyc jest",
    "test:watch": "nyc jest --watch",
    "test:cov": "nyc jest --coverage --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "nyc jest --config ./test/jest-e2e.json"
  },
  "heroku-run-build-script": true,
  "jest-junit": {
    "JEST_JUNIT_OUTPUT": "./test-results/jest"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-fastify": "^6.0.0",
    "@nestjs/typeorm": "^6.0.0",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "serve-favicon": "^2.5.0",
    "typeorm": "^0.2.15",
    "typescript": "^3.4.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/joi": "^14.3.2",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "jest-junit": "^6.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslib": "^1.9.3",
    "tslint": "5.12.1"
  }
}
